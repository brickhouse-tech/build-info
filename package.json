{
  "name": "@znemz/build-info",
  "version": "0.1.1",
  "description": "All things build information! git, os, artifacts times",
  "repository": "git@github.com:nmccready/build-info.git",
  "license": "MIT",
  "author": "Nicholas McCready",
  "files": [
    "lib",
    "src/*.js",
    "!src/*.spec.js"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn gulp",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "example": "node -r esm examples/basic.js",
    "gulp": "node -r esm ./node_modules/.bin/gulp",
    "jest": "node -r esm ./node_modules/.bin/jest",
    "lint": "esw *.js __jest__ --color",
    "lint:watch": "yarn lint --watch",
    "prepare": "yarn sort-package-json",
    "prepublish": "yarn test && yarn build",
    "test": "yarn jest",
    "test:ci": "yarn test --coverage",
    "preversion": "yarn test && yarn build"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "eslint": "^10.0.2",
    "eslint-config-gulp": "^6.0.0",
    "lodash": "^4.17.15",
    "macos-release": "^3.4.0",
    "os-name": "^7.0.0",
    "simple-git": "^3.32.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/node": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.29.0",
    "@babel/runtime": "^7.28.6",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.1",
    "del": "^8.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-jest": "^3.0.2",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-watch": "^8.0.0",
    "esm": "^3.2.22",
    "gulp": "5.0.1",
    "gulp-babel": "^8",
    "gulp-eslint": "^6.0.0",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^7.0.0",
    "sort-package-json": "^3.6.1"
  },
  "resolutions": {
    "minimatch": ">=10.2.1",
    "form-data": ">=2.5.4",
    "qs": ">=6.14.1",
    "tough-cookie": ">=4.1.3",
    "tmp": ">=0.2.4"
  }
}
